<MusicWindow>:
    MDBoxLayout:
        orientation: "vertical"
        MDScrollView:
            bar_width: 0
            height: "680dp"
            do_scroll_y: True
            effect_cls: "ScrollEffect"
            MDBoxLayout:
                orientation: "vertical"
                NavBar:
                    radius: [14]
                    elevation: 4
                    size_hint: .95, None
                    size_hint_x: "40dp"
                    md_bg_color: 1, 1, 1, 1
                    pos_hint: {"center_x": .5, "center_y": .07}


                NavBar:
                    elevation: 4
                    size_hint: 1, None
                    size_hint_x: "20dp"
                    md_bg_color: 1, 1, 1, .1
                    radius: [0, 0, 30, 30]
                    pos_hint: {"center_x": .5, "center_y": .07}
                    MDGridLayout:
                        cols: 5
                        spacing: "17dp"
                        pos_hint: {"center_x": .5, "center_y": .55}
                        MDIconButton:
                            id: nav_icon1
                            icon: "home"
                            ripple_scale: 3
                            user_font_size: "50sp"
                            theme_text_color: "Custom"
                        MDIconButton:
                            id: nav_icon1
                            icon: "home"
                            ripple_scale: 0
                            user_font_size: "50sp"
                            theme_text_color: "Custom"
                        MDIconButton:
                            id: nav_icon1
                            icon: "home"
                            ripple_scale: 0
                            user_font_size: "70sp"
                            theme_text_color: "Custom"
                        MDIconButton:
                            id: nav_icon1
                            icon: "home"
                            ripple_scale: 0
                            user_font_size: "50sp"
                            theme_text_color: "Custom"
                        MDIconButton:
                            id: nav_icon1
                            icon: "home"
                            ripple_scale: 0
                            user_font_size: "50sp"
                            theme_text_color: "Custom"

