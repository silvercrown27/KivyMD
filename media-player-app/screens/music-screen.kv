<MusicWindow>:
    MDBoxLayout:
        orientation: "vertical"
        MDBoxLayout:
            adaptive_height: True
            orientation: "vertical"
            MDTopAppBar:
                md_bg_color: app.theme_cls.primary_color
                size_hint: 1, None
                height: "100dp"
                padding: [0, 0, 10, 10]
        MDBoxLayout:
            height: "500dp"


        MDBoxLayout:
            orientation: "vertical"
            MDBottomAppBar:

                MDTopAppBar:
                    title: "Title"
                    icon: "git"
                    type: "bottom"
                    left_action_items: [["menu", lambda x: x]]
                    mode: "center"
                    remove_notch: self


