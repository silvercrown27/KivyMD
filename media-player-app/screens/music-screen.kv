<MusicWindow>:
    MDBoxLayout:
        orientation: "vertical"
        spacing: "5dp"
        MDBoxLayout:
            size_hint: 1, None
            height: "60dp"
            pos_hint: {"top": 1}
            padding: "15dp"
            orientation: "horizontal"
            MDLabel:
                text: "Your Space"
                font_style: "H5"
            MDCard:
                size_hint: None, None
                size: "35dp", "35dp"
                md_bg_color: [0, 0, 0, 0]
                pos_hint: {"center_y": .5}
                FitImage:
                    source: "img/profile.jfif"
                    size_hint: .75, .85
                    radius: 36, 36, 36, 36
                    pos_hint: {"center_y": .5}

        MDTabs:
            id: tabs
            Tab:
                title: "Tracks"
                ScrollView:
                    do_scroll_y: True
                    bar_width: 0
                    MDList:
                        id: tracks

            Tab:
                title: "playlist"
                ScrollView:
                    do_scroll_y: True
                    bar_width: 0
                    MDBoxLayout:
                        id: playlists
                        orientation: "vertical"
                        adaptive_height: True
            Tab:
                title: "artists"
                ScrollView:
                    do_scroll_y: True
                    bar_width: 0
                    MDBoxLayout:
                        id: artists
                        orientation: "vertical"
                        adaptive_height: True
            Tab:
                title: "genres"
                ScrollView:
                    do_scroll_y: True
                    bar_width: 0
                    MDBoxLayout:
                        id: genres
                        orientation: "vertical"
                        adaptive_height: True


        MDBoxLayout:
            size_hint: 1, .1
            pos_hint: {"top": .2}
            spacing: root.width / 8
            MDIconButton:
                icon: "magnify"
            MDIconButton:
                icon: "play-circle-outline"
                on_press: app.wm.ids.WindowManager.current = "VideosWindow"
            MDIconButton:
                icon: "music-circle-outline"
                on_press: app.wm.ids.WindowManager.current = "MusicWindow"
            MDIconButton:
                icon: "home"
                on_press: app.wm.ids.WindowManager.current = "HomeScreen"

<Tab>