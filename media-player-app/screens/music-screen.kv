<MusicWindow>:
    MDBoxLayout:
        orientation: "vertical"
        spacing: "5dp"
        MDTopAppBar:
            title: "Your Music"
            orientation: "horizontal"
            MDCard:
                size_hint: None, None
                size: "35dp", "35dp"
                md_bg_color: [0, 0, 0, 0]
                pos_hint: {"center_y": .5}
                FitImage:
                    source: "img/profile.jfif"
                    size_hint: .75, .85
                    radius: 36, 36, 36, 36
                    pos_hint: {"center_y": .5}

        MDTabs:
            id: tabs
            Tab:
                title: "Tracks"
                ScrollView:
                    do_scroll_y: True
                    bar_width: 0
                    MDBoxLayout:
                        id: tracks
                        orientation: "vertical"
                        adaptive_height: True
            Tab:
                title: "playlist"
                ScrollView:
                    do_scroll_y: True
                    bar_width: 0
                    MDBoxLayout:
                        id: playlists
                        orientation: "vertical"
                        adaptive_height: True
            Tab:
                title: "artists"
                ScrollView:
                    do_scroll_y: True
                    bar_width: 0
                    MDBoxLayout:
                        id: artists
                        orientation: "vertical"
                        adaptive_height: True
            Tab:
                title: "genres"
                ScrollView:
                    do_scroll_y: True
                    bar_width: 0
                    MDBoxLayout:
                        id: genres
                        orientation: "vertical"
                        adaptive_height: True



        NavBar:
            radius: [14]
            elevation: 4
            size_hint: .95, None
            height: "50dp"
            md_bg_color: 1, 1, 1, 1
            pos_hint: {"center_x": .5}
            MDGridLayout:
                cols: 5
                spacing: "17dp"
                pos_hint: {"center_x": .5, "center_y": .55}
                MDIconButton:
                    id: nav_icon1
                    icon: "home"
                    ripple_scale: 3
                    user_font_size: "50sp"
                    theme_text_color: "Custom"
                MDIconButton:
                    id: nav_icon1
                    icon: "home"
                    ripple_scale: 0
                    user_font_size: "50sp"
                    theme_text_color: "Custom"
                MDIconButton:
                    id: nav_icon1
                    icon: "home"
                    ripple_scale: 0
                    user_font_size: "70sp"
                    theme_text_color: "Custom"
                MDIconButton:
                    id: nav_icon1
                    icon: "home"
                    ripple_scale: 0
                    user_font_size: "50sp"
                    theme_text_color: "Custom"
                MDIconButton:
                    id: nav_icon1
                    icon: "home"
                    ripple_scale: 0
                    user_font_size: "50sp"
                    theme_text_color: "Custom"

<Tab>